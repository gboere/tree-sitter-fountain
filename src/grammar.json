{
  "name": "fountain",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "title_page"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "boneyard"
              },
              {
                "type": "SYMBOL",
                "name": "section"
              },
              {
                "type": "SYMBOL",
                "name": "scene"
              },
              {
                "type": "SYMBOL",
                "name": "_element"
              }
            ]
          }
        }
      ]
    },
    "title_page": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_title_element"
      }
    },
    "_title_element": {
      "type": "PATTERN",
      "value": "[\\w ]+:.*((\\n\\t+|\\n[ ]{2,}).*)*"
    },
    "scene": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "(INT|EXT|EST|INT.?\\/EXT|I.?\\/E).?.*\\n\\n"
              },
              {
                "type": "PATTERN",
                "value": "\\..+\\n\\n"
              }
            ]
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "_element"
            }
          }
        ]
      }
    },
    "dialogue": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "dialogue_block"
        }
      }
    },
    "dialogue_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "character",
          "content": {
            "type": "SYMBOL",
            "name": "character"
          }
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "speech"
              },
              {
                "type": "SYMBOL",
                "name": "parenthetical"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "character": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "([A-Z. ]+|@.+)[ ]*(\\(.+\\))?\\^?\\n"
        }
      ]
    },
    "parenthetical": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "PATTERN",
        "value": "\\(.*\\)\\n"
      }
    },
    "speech": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "PATTERN",
        "value": ".*\\n"
      }
    },
    "action": {
      "type": "PREC",
      "value": -1,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "!.+"
            },
            {
              "type": "SYMBOL",
              "name": "_line"
            }
          ]
        }
      }
    },
    "transition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[A-Z ]+ TO:\\n\\n"
        },
        {
          "type": "PATTERN",
          "value": ">.+[^<]\\n\\n"
        }
      ]
    },
    "break": {
      "type": "PATTERN",
      "value": "={3,}\\n"
    },
    "synope": {
      "type": "PATTERN",
      "value": "=.+\\n"
    },
    "section": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "#{1,}.+\\n"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "scene"
                },
                {
                  "type": "SYMBOL",
                  "name": "_element"
                }
              ]
            }
          }
        ]
      }
    },
    "note": {
      "type": "PATTERN",
      "value": "\\[\\[.+\\]\\]\\n"
    },
    "boneyard": {
      "type": "PREC",
      "value": 10,
      "content": {
        "type": "PATTERN",
        "value": "\\/\\*(.|\\n)*\\*\\/"
      }
    },
    "_line": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "_element": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "action"
          },
          {
            "type": "SYMBOL",
            "name": "dialogue"
          },
          {
            "type": "SYMBOL",
            "name": "transition"
          },
          {
            "type": "SYMBOL",
            "name": "synope"
          },
          {
            "type": "SYMBOL",
            "name": "break"
          },
          {
            "type": "SYMBOL",
            "name": "note"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
